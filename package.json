{
  "name": "n8ncli",
  "version": "1.0.0",
  "description": "CLI for interacting with the n8n API",
  "type": "module",
  "bin": {
    "n8ncli": "./dist/bin.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "cli": "node --env-file=.env dist/bin.js",
    "cli:dev": "npm run build && npm run cli",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "keywords": [
    "n8n",
    "api",
    "automation",
    "workflow"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^22.0.0",
    "typescript": "^5.7.0",
    "vitest": "^4.0.17"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "chalk": "^5.6.2",
    "commander": "^14.0.2"
  }
}
